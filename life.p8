__lua__
-- game of life
-- by ðŸ˜

d=48
off=(128-d)/2

-- wrap coordinate
function wrap(v)
 if (v > d) return v - d
 if (v < 0) return v + d
 return v
end

function psetw(x,y,c)
 pset(wrap(x)+off,wrap(y)+off,c)
end

function pgetw(x,y)
 return pget(wrap(x)+off,wrap(y)+off)
end

function tocoord(idx)
 return idx%d,idx/d
end

function _init()
 for i=1,d*d do
  local x,y=tocoord(i-1)
  if (rnd(3)>2) sset(x,y,7)
 end
end

function n_at(x,y)
 return band(1,pgetw(x,y))
end

function n_cnt(x,y)
  local cnt = 0
  cnt+=n_at(x-1,y-1)
  cnt+=n_at(x,y-1)
  cnt+=n_at(x+1,y-1)
  cnt+=n_at(x-1,y)
  cnt+=n_at(x+1,y)
  cnt+=n_at(x-1,y+1)
  cnt+=n_at(x,y+1)
  cnt+=n_at(x+1,y+1)
  return cnt
end

function _draw()
 cls()
 spr(0,off,off,d,d)
 for i=1,d*d do
  local x,y=tocoord(i-1)
 	local alive = n_at(x,y)>0
  local ncnt = n_cnt(x,y)
  if ncnt==3 or (alive and ncnt==2) then
   sset(x,y,7)
  else
   sset(x,y,0)
  end
 end
end
