 add(inter, grave(27,13,
  "jETHRO",
  "dRAINED BY A VAMPIRE"))
  
 temp_text(64,56,"gRAVE mATTERS",13)
  :add_mod(cam_rel())
 temp_text(64,64,"üÖæÔ∏è=act ‚ùé=jump",13)
  :add_mod(cam_rel())

 -- test rect
 x1r=0 y1r=0 x2r=0 y2r=0
end
