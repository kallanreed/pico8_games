pico-8 cartridge // http://www.pico-8.com
version 33
__lua__
function _init()
 x=0 y1=0 y2=0
 cls()
end

function _draw()
	y1+=cos(t())
	y2+=sin(t())
	
	pset(x,30,8)
	pset(x,y1+30,8)
	
	pset(x,70,9)
	pset(x,y2+70,9)
	
	x+=1
	if x>127 then
	 x=0
	 cls()
	end
end
