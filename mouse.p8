pico-8 cartridge // http://www.pico-8.com
version 33
__lua__
-- disco mouse
-- kallanreed

--music(0,0,7)
--150.615bpm
sec_per_beat=.39837

arena={
 t=1,b=28,l=1,r=30
}

board={
 icons={35,34},
 data={},
 
 at=function(my,x,y)
  return my.data[px2b(x,y)]
 end,
 
 draw=function(my)
  for i=0,#my.data-1 do
   -- offset math works best
   -- with offset 0, adjust later
   local x,y=i%30,i\30
   local sp=my.icons[my.data[i+1]]
   -- +4 to account for the edges
   if (sp) spr(sp,x*4+4,y*4+4)
  end
 end,
 
 load_map=function(my,sp)
  local offx,offy=
   8*(sp-(sp\8*8)),8*sp\16
  local i=1
  -- only read out 30x28 center
  for r=1,28 do
   for c=1,30 do
    my.data[i]=sget(offx+c,offy+r)
    i+=1
   end
  end
  -- todo use the bottom rows
  -- as metadata
 end,
 
 push=function(my,x,y,dir)
  local dstx,dsty=x,y
  if (dir==⬅️) dstx-=1
  if (dir==➡️) dstx+=1
  if (dir==⬆️) dsty-=1
  if (dir==⬇️) dsty+=1
  
  local dst=my:at(dstx,dsty)
  -- todo bounds
  if (dst==1
   and my:push(dstx,dsty,dir))
   or dst==0 then
   my.data[px2b(dstx,dsty)]=my:at(x,y)
   my.data[px2b(x,y)]=0
   return true
  end
  
  out("no push")
  return false
 end
}

player={
 x=15,y=14,
 draw=function(my)
  spr(16,my.x*4,my.y*4)
 end,
 update=function(my)
  local nx,ny,dir=my.x,my.y,nil

  if btnp(⬆️) then
   ny-=1 dir=⬆️
  elseif btnp(⬇️) then
   ny+=1 dir=⬇️
  elseif btnp(⬅️) then
   nx-=1 dir=⬅️
  elseif btnp(➡️) then
   nx+=1 dir=➡️
  end
  -- todo, just let 'push'
  -- handle bounds check
  nx=mid(arena.l,nx,arena.r)
  ny=mid(arena.t,ny,arena.b)
  
  if dir!=nil
  and board:push(my.x,my.y,dir) then
   my.x=nx my.y=ny
  end
 end
}

cat={
 x=0,y=0,
 new=function(my,o)
  o=o or {}
  setmetatable(o,my)
  o.x,o.y=
   ceil(rnd(arena.r)),
   ceil(rnd(arena.b))
  return o
 end,
 update=function(my)
  local dx,dy=
   player.x-my.x,
   player.y-my.y  
  local nx,ny=
   my.x+sgnz(dx),
   my.y+sgnz(dy)
  if board:at(nx,ny)==0 then
   my.x=nx
   my.y=ny
  end
 end,
 draw=function(my)
  spr(32,my.x*4,my.y*4)
 end
}
cat.__index=cat
cats={}

score={
 val=0,
 draw=function(my)
  fillp()
  print("\#0score "..my.val,1,121,7)
 end
}

lives={
 val=3,
 draw=function(my)
  for i=1,my.val do
   spr(16,127-i*6,122)
  end
 end
}

pals={
{[1]=1,[12]=12,[2]=2,[8]=8},
{[1]=2,[12]=8,[2]=3,[8]=11},
{[1]=3,[12]=11,[2]=9,[8]=10},
{[1]=9,[12]=10,[2]=1,[8]=12},
}

function _init()
 pal_ix=1
 pal_beat=0
 cat_beat=0
 beat_ani=0
 add(cats,cat:new())
 add(cats,cat:new())
 board:load_map(64)
end

function _update()
 player:update()
 
 if t()-beat_ani>sec_per_beat then
  on_beat()
 end
end

function on_beat()
 beat_ani=t()
 
 cat_beat+=1
 if cat_beat>1 then
  cat_beat=0
 foreach(cats,do_update)
 end

 pal_beat+=1
 if pal_beat>1 then
  pal_beat=0
  pal_ix+=1
  if (pal_ix>#pals) pal_ix=1
 end
end

function _draw()
 cls()
 
 for r=arena.t,arena.b do
  for c=arena.l,arena.r do
   if (band(r,1)^^band(c,1))==1 then
    local x,y=c*4,r*4
    fillp(0xdfff)
    rectfill(x,y,x+3,y+3,1)
   end
  end
 end
 
 pal(pals[pal_ix])
 map(0,0)
 pal()
 
 board:draw()
 foreach(cats, do_draw)
 score:draw()
 lives:draw()
 player:draw()
end

-->8
-- utils

function do_update(o)
 o:update()
end

function do_draw(o)
 o:draw()
end

function px2b(x,y)
 return (y-arena.t)*30+x
end

function sgnz(x)
 return _if(x==0,0,sgn(x))
end

-- in lieu of conditional
function _if(c,vt,vf)
 if (c) return vt
 return vf
end

function out(...)
 local str=""
 for i in all({...}) do
  str=str.." "..i
 end
 printh(str)
end
